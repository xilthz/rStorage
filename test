import java.util.Scanner;

public class test{
  Scanner sc = new Scanner(System.in);
    public static void main(String []args){
     int x1 = sc.nextInt();
     int x2 = sc.nextInt();
     int y1 = sc.nextInt();
     int y2 = sc.nextInt();
  
     String res = "";
     double calculating = Math.sqrt(Math.pow(x2-x1) + Math.pow(y2-y1));
     return calculating;
     }
}
